<?xml version="1.0" encoding="utf-8" ?>
<toolkit:Popup xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MiracleApp.Pages.LoadingPage"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             Opened="Popup_Opened"
             CanBeDismissedByTappingOutsideOfPopup="False">
    
    <Grid BackgroundColor="#242323" Padding="25" Margin="0" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
        <Image BackgroundColor="#242323" x:Name="image" HorizontalOptions="Center" VerticalOptions="Center" ZIndex="0"/>
        
    </Grid>
</toolkit:Popup>